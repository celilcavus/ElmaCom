@model IEnumerable<BaseCategory>

<table class="table table-hover table-sprited">
    <thead>
        <tr>
            <td>ID</td>
            <td>Category Name</td>
            <td>Description</td>
            <td>Create Date</td>
            <td>Update Date</td>
            <td>Is Active</td>
            <td>Update</td>
            <td>Delete</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.CategoryName</td>
                <td>@item.Description</td>
                <td>@item.CreateDate</td>
                <td>@item.LastUpdateDate</td>
                <td>@item.IsActive</td>
                <td><a asp-action="Update" asp-controller="BaseCategory" asp-route-id="@item.Id" class="btn btn-sm btn-warning">Update</a></td>
                <td><a asp-action="Delete" asp-controller="BaseCategory" asp-route-id="@item.Id" class="btn btn-sm btn-danger">Delete</a></td>
            </tr>
        }
    </tbody>
</table>
<br />
<a asp-action="Index" asp-controller="BaseCategory" class="btn btn-outline-dark">Back To Base Category</a>